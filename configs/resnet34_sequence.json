{
    "model": {
        "encoder": {
            "name": "resnet34"
        },
        "sequence_model": {
            "hidden_size": 256,
            "num_layers": 1
        }
    },
    "training": {
        "num_epochs": 50,
        "log_iterations": 20,
        "gpu_id": 0
    },
    "dataset": {
        "dataset_file": "trainval_set.json",
        "predict_speed": false
    },
    "data_loader": {
        "batch_size": 4,
        "num_workers": 2
    },
    "lr_scheduler": {
        "initial_lr": 1e-3
    },
    "optimizer": {
        "name": "Adam",
        "lr": 4e-3
    }
}